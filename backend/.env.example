# Database Configuration
# Format: postgresql+asyncpg://user:password@host:port/database?ssl=require
# Note: Use 'ssl=require' (not 'sslmode=require') for asyncpg driver
DATABASE_URL=postgresql+asyncpg://neondb_owner:your_password@your-host.neon.tech/neondb?ssl=require

# CORS Configuration (for future frontend integration)
CORS_ORIGINS=["http://localhost:3000"]

# Logging
LOG_LEVEL=INFO
